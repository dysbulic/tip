$(function() {
	var lineHeight = 22
	var $top = $(document.createElement('clipPath'))
	$top.attr({ id: 'top' })
	console.log($top)
	$('defs').append($top)
	for(var count = 0; count < 10; count++) {
		var $line = $(document.createElement('rect'))
		$line.attr({
			x: 0,
			y: count * lineHeight * 2,
			width: '100%',
			height: lineHeight
		})
		$top.append($line)
	}
//      <rect x="0" y="35" width="400" height="22"/>
//      <rect x="0" y="75" width="400" height="22"/>
})
