	<entry>
			<subject>Resizing</subject>
			<event>&lt;p&gt;Script du jour: any images larger than 640x480 should be sized down to fit within those bounds:&lt;/p&gt;

&lt;code&gt;&lt;pre&gt;W=640;
H=480;
for file in *; do
  info=$(identify "$file" | sed -e 's/.* \([[:digit:]]\+\)x\([[:digit:]]\+\) .*$/\1x\2/');
  if [[ ${info%x*} -gt $W || ${info#*x} -gt $H ]]; then
    echo "Resizing: $file";
    ext=${file##*.};
    convert -resize ${W}x${H} "$file" "${file%$ext}sized.$ext";
  fi;
done&lt;/pre&gt;&lt;/code&gt;</event>
