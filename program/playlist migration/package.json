{
  "name": "music-playlist-exporters",
  "version": "1.0.0",
  "description": "Export Spotify and YouTube Music playlists to JSON and CSV formats",
  "main": "spotify-export.ts",
  "scripts": {
    "spotify": "ts-node spotify-export.ts",
    "youtube": "ts-node youtube-export.ts",
    "import": "ts-node youtube-import.ts",
    "build": "tsc",
    "dev": "ts-node-dev --respawn spotify-export.ts",
    "finish": "deno -A youtube-import.ts 'Revolution #7.Spotify.csv' 'Revolution #7.YouTube.copied.csv' -p 'Import #7'"
  },
  "keywords": [
    "spotify",
    "youtube",
    "youtube-music",
    "playlist",
    "export",
    "api"
  ],
  "author": "",
  "license": "CC0-1.0",
  "dependencies": {
    "dotenv": "^16.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.24",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}